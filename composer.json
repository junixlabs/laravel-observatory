{
    "name": "junixlabs/laravel-observatory",
    "description": "Observability toolkit for Laravel applications - Monitor HTTP requests, outbound calls, queue jobs with Prometheus and SidMonitor support",
    "keywords": ["laravel", "monitoring", "observability", "prometheus", "metrics", "apm", "logging"],
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "JunixLabs",
            "email": "dev@junixlabs.com"
        }
    ],
    "homepage": "https://github.com/junixlabs/be-observatory",
    "support": {
        "issues": "https://github.com/junixlabs/be-observatory/issues",
        "source": "https://github.com/junixlabs/be-observatory"
    },
    "require": {
        "php": "^8.1",
        "illuminate/support": "^10.0|^11.0",
        "illuminate/http": "^10.0|^11.0",
        "illuminate/routing": "^10.0|^11.0",
        "illuminate/queue": "^10.0|^11.0",
        "promphp/prometheus_client_php": "^2.6"
    },
    "require-dev": {
        "orchestra/testbench": "^8.0|^9.0",
        "phpunit/phpunit": "^10.0|^11.0"
    },
    "autoload": {
        "psr-4": {
            "JunixLabs\\Observatory\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "JunixLabs\\Observatory\\Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "JunixLabs\\Observatory\\ObservatoryServiceProvider"
            ],
            "aliases": {
                "Observatory": "JunixLabs\\Observatory\\Facades\\Observatory"
            }
        }
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "stable",
    "prefer-stable": true
}
